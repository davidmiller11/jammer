<!-- new.html.erb - logins -->
<h2>Sign in</h2>
<div class="main-wrapper">
<%= form_tag login_path do %>
  
  <p>
    Email<br />
    <%= email_field_tag :email %>
  </p>
  <p>
    Password<br />
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag :login %>
  </p>
<% end %>
</div>